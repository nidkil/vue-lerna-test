{
  "name": "@vue-lerna-test/example-ui",
  "version": "0.0.0",
  "scripts": {
    "serve:ui": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint -c .eslintrc.js --format codeframe src tests",
    "lint:fix": "vue-cli-service lint --fix -c .eslintrc.js --format codeframe src tests",
    "lint:error-only": "vue-cli-service lint -c .eslintrc.js --quiet --format codeframe src tests",
    "test": "npm run test:unit",
    "test:unit": "vue-cli-service test:unit",
    "cz:commit": "git cz",
    "cz:retry": "git cz --retry"
  },
  "dependencies": {
    "@vue-lerna-test/plugin": "^0.0.0",
    "vue": "^2.5.17",
    "vue-router": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.4",
    "@vue/cli-plugin-eslint": "^3.8.0",
    "@vue/cli-plugin-unit-jest": "^3.0.4",
    "@vue/cli-service": "^3.0.4",
    "@vue/eslint-config-prettier": "^4.0.0",
    "@vue/test-utils": "^1.0.0-beta.20",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0-0",
    "vue-template-compiler": "^2.5.17"
  }
}
